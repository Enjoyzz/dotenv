{
  "name": "enjoys/dotenv",
  "description": "Registers environment variables from a .env file",
  "keywords": [
    "environment",
    "env",
    "dotenv"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Enjoys\\Dotenv\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "webmozart/assert": "^1.10",
    "symfony/polyfill-php80": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.10",
    "infection/infection": "^0.25"
  },
  "scripts": {
    "psalm": "psalm",
    "infection": "infection --configuration=infection.json",
    "test": "phpunit"
  }
}
