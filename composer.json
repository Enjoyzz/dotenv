{
    "name": "enjoys/dotenv",
    "description": "Registers environment variables from a .env file",
    "keywords": [
        "environment",
        "env",
        "dotenv"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Enjoys\\Dotenv\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.10",
        "infection/infection": "^0.26",
        "symfony/var-dumper": "^v5.4",
        "phpmetrics/phpmetrics": "^v2.8"
    },
    "scripts": {
        "psalm": "psalm",
        "infection": "XDEBUG_MODE=coverage infection -j2 --configuration=infection.json",
        "test": "phpunit",
        "static": [
            "@test",
            "@psalm"
        ]
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
