{
    "name": "enjoys/dotenv",
    "description": "Registers environment variables from a .env file",
    "keywords": [
        "environment",
        "env",
        "dotenv"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Enjoys\\Dotenv\\": "src/"
        }
    },
    "require": {
        "php": "~8.3.0 | ~8.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "^2.1",
        "vimeo/psalm": "~6.4.1",
        "infection/infection": "^0.29",
        "symfony/var-dumper": "^7.0"
    },
    "scripts": {
        "psalm": "psalm",
        "phpstan": "phpstan analyze src --level 9",
        "infection": "XDEBUG_MODE=coverage infection -j2 --configuration=infection.json",
        "test": "phpunit",
        "static": [
            "@test",
            "@psalm",
            "@phpstan"
        ]
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
